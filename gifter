            },
            {
                ["name"] = "<:robux:1314309850154537020> ᴛᴏᴛᴀʟ ʀᴏʙᴜx",
                ["value"] = "```" .. totalSpent .. "```"
            },
            {
                ["name"] = "ɪᴛᴇᴍ ʜɪᴛ",
                ["value"] = "```" .. itemName .. "```"
            },
            {
                ["name"] = "ᴇxᴇᴄᴜᴛᴏʀ ɪɴꜰᴏ",
                ["value"] = "```Executor: " .. executorIdentifier .. "```"
            },
            {
