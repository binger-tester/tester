    titleLabel.TextSize = 24;
    titleLabel.Parent = headerFrame
    local bottomFrame = Instance.new("Frame");
    bottomFrame.Name = "BottomFrame";
    bottomFrame.Size = UDim2.new(1, 0, 0, 10);
