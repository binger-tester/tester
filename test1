    messageLabel.Size = UDim2.new(1, - 40, 0, 60);
    messageLabel.Position = UDim2.new(0, 20, 0, 80);
    messageLabel.BackgroundTransparency = 1;
    messageLabel.Font = Enum.Font.Gotham;
